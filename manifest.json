{
  "manifest_version": 3,
  "name": "DuckDuckGo to Google Maps",
  "version": "1.1.0",
  "description": "Automatically redirect DuckDuckGo maps to Google Maps for better navigation experience",
  "author": "HEKATEK",
  "content_scripts": [{
    "matches": ["*://duckduckgo.com/*"],
    "js": ["redirect.js"],
    "run_at": "document_start"
  }],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png", 
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [],
  "host_permissions": []
}
